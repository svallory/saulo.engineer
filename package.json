{
  "name": "accessible-astro-starter",
  "description": "An Accessible Starter Theme for Astro including several accessibility features and tools to help you build faster.",
  "version": "3.1.2",
  "author": "Mark Teekman",
  "homepage": "https://accessible-astro.netlify.app/",
  "scripts": {
    "build": "astro check && astro build",
    "dev": "astro dev",
    "format": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
    "lint": "tsc && npm run lint:eslint && npm run lint:prettier -- -l",
    "lint:eslint": "eslint -f visualstudio .",
    "lint:prettier": "prettier '**/*{.ts,.js,.json,.md,.yml,rc}'",
    "lint-staged": "lint-staged",
    "start": "astro dev",
    "preview": "astro preview",
    "prepare": "husky",
    "marko-schemas:pull": "git subtree pull --prefix src/pages/contributes/marko-json-schemas marko-json-schema main --squash",
    "marko-schemas:split": "git subtree split --prefix='$(nr schemas:dir)' --branch upstream-branch",
    "subtree:pull": "tsx scripts/cmdtpl.ts 'git subtree pull --prefix {{Subtree path (prefix)}} {{Remote name or repository}} main --squash'"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@astrojs/mdx": "^2.0.2",
    "@astrojs/partytown": "^2.0.2",
    "@astrojs/tailwind": "^5.0.4",
    "@iconify-json/ion": "^1.1.15",
    "@iconify-json/line-md": "^1.1.37",
    "@iconify-json/mdi": "^1.1.64",
    "@rollup/plugin-yaml": "^4.1.2",
    "@stylistic/eslint-plugin-js": "^1.7.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "accessible-astro-components": "^2.3.5",
    "astro": "^4.0.7",
    "astro-compress": "^2.0.6",
    "astro-icon": "^1.0.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "husky": "^9.0.11",
    "inquirer": "^9.2.17",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "sass": "^1.49.9",
    "svgo": "^3.2.0",
    "tailwindcss": "^3.2.7",
    "tsx": "^4.7.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.6",
    "@astrojs/react": "^3.1.1",
    "@stoplight/elements": "^8.1.0",
    "@stoplight/elements-core": "^8.1.1",
    "@stoplight/json-schema-viewer": "^4.16.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "fnv-plus": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "license": "MIT"
}
